{
  "id": "nextjs-dashboard",
  "name": "Next.js Dashboard",
  "description": "A Next.js dashboard template with data visualization and admin features",
  "version": "1.0.0",
  "tags": ["nextjs", "dashboard", "admin", "charts"],
  "screenshot": null,
  "base_command": "npx create-next-app@latest",
  "recommended_modules": ["tailwind", "daisyui", "state", "i18n"],
  "structure": {
    "enforced": true,
    "directories": [
      "src/app",
      "src/app/dashboard",
      "src/app/dashboard/(views)",
      "src/app/dashboard/analytics",
      "src/app/dashboard/settings",
      "src/components",
      "src/components/dashboard",
      "src/components/charts",
      "src/components/ui",
      "src/lib",
      "src/lib/api",
      "src/lib/store",
      "src/lib/utils",
      "src/styles",
      "src/types",
      "public"
    ]
  }
} 